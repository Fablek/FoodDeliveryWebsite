<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - FoodDeliveryWebsite</title>
    <script type="importmap"></script>
    <link href="~/css/style.css" rel="stylesheet">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
</head>
<body>
    <header class="max-w-(--container-width) mx-auto mb-[40px]">
        <div class="flex justify-between items-center bg-(--gray-color) border-r border-b border-l border-(--black-10) rounded-b-2xl">
            <div class="flex items-center gap-[12px] pl-[30px]">
                <span>🌟</span>
                <span class="font-medium text-[15px]">Get 5% Off your first order, <span class="font-semibold text-(--orange) underline decoration-1">Promo: ORDER5</span></span>
            </div>
            <div class="flex items-center gap-[70px]">
                <div class="flex items-center gap-[14px]">
                    <svg width="25" height="25" viewBox="0 0 25 25" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                        <rect width="25" height="25" fill="url(#pattern0_33_51)" />
                        <defs>
                            <pattern id="pattern0_33_51" patternContentUnits="objectBoundingBox" width="1" height="1">
                                <use xlink:href="#image0_33_51" transform="scale(0.0111111)" />
                            </pattern>
                            <image id="image0_33_51" width="90" height="90" preserveAspectRatio="none" xlink:href="data:image/png;base64," />
                        </defs>
                    </svg>
                    <span class="font-medium text-[15px]">Regent Street, A4, A4201, London</span>
                    <span class="font-medium text-[15px] text-(--orange) underline decoration-1">Change Location</span>
                </div>
                <div class="flex items-center bg-(--green-color) rounded-b-2xl">
                    <span class="px-[21px] py-[13px] border-r border-(--white-30)">
                        <svg width="43" height="43" viewBox="0 0 43 43" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                            <rect width="43" height="43" fill="url(#pattern0_33_259)" />
                            <defs>
                                <pattern id="pattern0_33_259" patternContentUnits="objectBoundingBox" width="1" height="1">
                                    <use xlink:href="#image0_33_259" transform="scale(0.0111111)" />
                                </pattern>
                                <image id="image0_33_259" width="90" height="90" preserveAspectRatio="none" xlink:href="data:image/png;base64," />
                            </defs>
                        </svg>
                    </span>
                    <span class="py-[23px] px-[21px] border-r border-(--white-30) text-white font-semibold">23 Items</span>
                    <span class="py-[23px] px-[21px] text-white font-semibold">GBP 79.89</span>
                    <span class="pl-[10px] pr-[14px] py-[15px] border-l border-(--white-30) border-opacity-30">
                        <svg width="38" height="38" viewBox="0 0 38 38" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                            <rect x="38" width="38" height="38" transform="rotate(90 38 0)" fill="url(#pattern0_33_265)" />
                            <defs>
                                <pattern id="pattern0_33_265" patternContentUnits="objectBoundingBox" width="1" height="1">
                                    <use xlink:href="#image0_33_265" transform="scale(0.0111111)" />
                                </pattern>
                                <image id="image0_33_265" width="90" height="90" preserveAspectRatio="none" xlink:href="data:image/png;base64," />
                            </defs>
                        </svg>
                    </span>
                </div>
            </div>
        </div>

        <div class="flex justify-between items-center gap-4 mt-[40px]">
            <a class="navbar-brand" asp-area="" asp-page="/Index">
                <svg width="215" height="53" viewBox="0 0 215 53" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                    <rect width="215" height="53" fill="url(#pattern0_33_41)" />
                    <defs>
                        <pattern id="pattern0_33_41" patternContentUnits="objectBoundingBox" width="1" height="1">
                            <use xlink:href="#image0_33_41" transform="matrix(0.00230385 0 0 0.00934579 0.018496 0)" />
                        </pattern>
                        <image id="image0_33_41" width="418" height="107" preserveAspectRatio="none" xlink:href="data:image/png;base64," />
                    </defs>
                </svg>
            </a>
            @{
                string currentPage = Model.PageContext.ActionDescriptor.DisplayName;
            }
            <nav>
                <ul class="p-0 m-0 flex justify-between items-center gap-x-4">
                    <li>
                        <a class="@("nav-item " + ((currentPage == "/Index") ? "nav-item-active" : ""))" asp-area="" asp-page="/Index">Home</a>
                    </li>
                    <li>
                        <a class="@("nav-item " + ((currentPage == "/Menu") ? "nav-item-active" : ""))" asp-area="" asp-page="/Menu">Browse Menu</a>
                    </li>
                    <li>
                        <a class="@("nav-item " + ((currentPage == "/SpecialOffers") ? "nav-item-active" : ""))" asp-area="" asp-page="/SpecialOffers">Special Offers</a>
                    </li>
                    <li>
                        <a class="@("nav-item " + ((currentPage == "/Restaurants") ? "nav-item-active" : ""))" asp-area="" asp-page="/Restaurants">Restaurants</a>
                    </li>
                    <li>
                        <a class="@("nav-item " + ((currentPage == "/TrackOrder") ? "nav-item-active" : ""))" asp-area="" asp-page="/TrackOrder">Track Order</a>
                    </li>
                </ul>
            </nav>
            <a class="flex items-center gap-[12px] text-white no-underline text-base font-medium rounded-[120px] bg-(--dark) px-[26px] py-[17px] transition-colors transition-[bg] duration-200 ease-in-out hover:text-(--orange)" asp-area="" asp-page="/">
                <svg width="32" height="27" viewBox="0 0 32 27" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                    <rect x="0.382353" width="30.9706" height="27" rx="1" fill="url(#pattern0_33_47)" />
                    <defs>
                        <pattern id="pattern0_33_47" patternContentUnits="objectBoundingBox" width="1" height="1">
                            <use xlink:href="#image0_33_47" transform="matrix(0.00968661 0 0 0.0111111 0.0641026 0)" />
                        </pattern>
                        <image id="image0_33_47" width="90" height="90" preserveAspectRatio="none" xlink:href="data:image/png;base64," />
                    </defs>
                </svg>
                <span class="no-underline">Login/Signup</span>
            </a>
        </div>
    </header>

    <div>
        <main role="main">
            @RenderBody()
        </main>
    </div>

