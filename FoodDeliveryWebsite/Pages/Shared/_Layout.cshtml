<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - FoodDeliveryWebsite</title>
    <script type="importmap"></script>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link href="~/css/style.css" rel="stylesheet">
</head>
<body>
    <header class="max-w-(--container-width) mx-auto">
        <div class="flex justify-between items-center bg-(--gray-color)">
            <div class="flex items-center gap-[12px]">
                <span>🌟</span>
                <span>Get 5% Off your first order, <span>Promo: ORDER5</span></span>
            </div>
            <div class="flex items-center gap-[70px]">
                <div class="flex items-center gap-[14px]">
                    <svg width="25" height="25" viewBox="0 0 25 25" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                        <rect width="25" height="25" fill="url(#pattern0_33_51)" />
                        <defs>
                            <pattern id="pattern0_33_51" patternContentUnits="objectBoundingBox" width="1" height="1">
                                <use xlink:href="#image0_33_51" transform="scale(0.0111111)" />
                            </pattern>
                            <image id="image0_33_51" width="90" height="90" preserveAspectRatio="none" xlink:href="data:image/png;base64," />
                        </defs>
                    </svg>
                    <span>Regent Street, A4, A4201, London</span>
                    <span>Change Location</span>
                </div>
                <div class="flex items-center bg-(--green-color)">
                    <svg class="" width="43" height="43" viewBox="0 0 43 43" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                        <rect width="43" height="43" fill="url(#pattern0_33_259)" />
                        <defs>
                            <pattern id="pattern0_33_259" patternContentUnits="objectBoundingBox" width="1" height="1">
                                <use xlink:href="#image0_33_259" transform="scale(0.0111111)" />
                            </pattern>
                            <image id="image0_33_259" width="90" height="90" preserveAspectRatio="none" xlink:href="data:image/png;base64," />
                        </defs>
                    </svg>
                    <span class="">23 Items</span>
                    <span class="">GBP 79.89</span>
                    <svg class="" width="38" height="38" viewBox="0 0 38 38" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                        <rect x="38" width="38" height="38" transform="rotate(90 38 0)" fill="url(#pattern0_33_265)" />
                        <defs>
                            <pattern id="pattern0_33_265" patternContentUnits="objectBoundingBox" width="1" height="1">
                                <use xlink:href="#image0_33_265" transform="scale(0.0111111)" />
                            </pattern>
                            <image id="image0_33_265" width="90" height="90" preserveAspectRatio="none" xlink:href="data:image/png;base64," />
                        </defs>
                    </svg>
                </div>
            </div>
        </div>

        <div class="flex justify-between items-center gap-4 mt-[40px]">
            <a class="navbar-brand" asp-area="" asp-page="/Index">
                <svg width="215" height="53" viewBox="0 0 215 53" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                    <rect width="215" height="53" fill="url(#pattern0_33_41)" />
                    <defs>
                        <pattern id="pattern0_33_41" patternContentUnits="objectBoundingBox" width="1" height="1">
                            <use xlink:href="#image0_33_41" transform="matrix(0.00230385 0 0 0.00934579 0.018496 0)" />
                        </pattern>
                        <image id="image0_33_41" width="418" height="107" preserveAspectRatio="none" xlink:href="data:image/png;base64," />
                    </defs>
                </svg>
            </a>
            <nav class="">
                <ul class="p-0 m-0 flex justify-between items-center gap-x-16">
                    <li class="">
                        <a class="" asp-area="" asp-page="/Index">Home</a>
                    </li>
                    <li class="">
                        <a class="" asp-area="" asp-page="/">Browse Menu</a>
                    </li>
                    <li class="">
                        <a class="" asp-area="" asp-page="/">Special Offers</a>
                    </li>
                    <li class="">
                        <a class="" asp-area="" asp-page="/">Restaurants</a>
                    </li>
                    <li class="">
                        <a class="" asp-area="" asp-page="/">Track Order</a>
                    </li>
                </ul>
            </nav>
            <a class="flex items-center gap-[12px]" asp-area="" asp-page="/">
                <svg width="32" height="27" viewBox="0 0 32 27" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                    <rect x="0.382353" width="30.9706" height="27" rx="1" fill="url(#pattern0_33_47)" />
                    <defs>
                        <pattern id="pattern0_33_47" patternContentUnits="objectBoundingBox" width="1" height="1">
                            <use xlink:href="#image0_33_47" transform="matrix(0.00968661 0 0 0.0111111 0.0641026 0)" />
                        </pattern>
                        <image id="image0_33_47" width="90" height="90" preserveAspectRatio="none" xlink:href="data:image/png;base64," />
                    </defs>
                </svg>
                <span>Login/Signup</span>
            </a>
        </div>
    </header>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2025 - FoodDeliveryWebsite - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
